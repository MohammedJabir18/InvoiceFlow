{
  "productName": "InvoiceFlow",
  "version": "0.1.0",
  "identifier": "com.invoiceflow.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "InvoiceFlow",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "decorations": true,
        "center": true,
        "minWidth": 1024,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDU1QTIwMEYxMjZDNEFBQzcKUldUSHFzUW04UUNpVlVBNUNUUEVqMm8ySHpybG41M1ZoSWh6b21TdFB5aWc5bkFqRldTUjRQbGMK",
      "endpoints": [
        "https://github.com/MohammedJabir18/InvoiceFlow/releases/latest/download/latest.json"
      ]
    }
  }
}